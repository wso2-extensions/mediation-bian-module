<?xml version="1.0" encoding="UTF-8"?>
<endpoint name="PaymentBackendPositionKeeping" xmlns="http://ws.apache.org/ns/synapse">
    <http method="put" uri-template="http://localhost:8290/PaymentBackend/PositionKeeping/{uri.var.positionkeepingid}/Update">
        <suspendOnFailure>
            <initialDuration>-1</initialDuration>
            <progressionFactor>1.0</progressionFactor>
        </suspendOnFailure>
        <markForSuspension>
            <retriesBeforeSuspension>0</retriesBeforeSuspension>
        </markForSuspension>
    </http>
</endpoint>
